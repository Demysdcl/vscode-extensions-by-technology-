{
	"name": "quarkus-snippets",
	"displayName": "Quarkus snippets",
	"description": "Quarkus snippets",
	"license": "Apache-2.0",
	"version": "1.2.0",
	"icon": "images/logo.png",
	"publisher": "ggrebert",
	"author": "Geoffrey GREBERT",
	"repository": {
		"type": "git",
		"url": "https://gitlab.com/geoffrey-grebert/vscode-quarkus-snippets"
	},
	"bugs": "https://gitlab.com/geoffrey-grebert/vscode-quarkus-snippets/issues",
	"galleryBanner": {
		"color": "#d8ebff",
		"theme": "light"
	},
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Snippets"
	],
	"keywords": [
		"java",
		"quarkus",
		"maven",
		"Quarkus properties",
		"quarkus-properties",
		"docker-compose",
		"containers",
		"dev container"
	],
	"main": "./dist/extension.js",
	"activationEvents": [
		"onCommand:extension.checkQuarkusUpdate",
		"onCommand:extension.initDevContainer"
	],
	"contributes": {
		"commands": [
			{
				"command": "extension.checkQuarkusUpdate",
				"title": "Quarkus: Check for update"
			},
			{
				"command": "extension.initDevContainer",
				"title": "Quarkus: Add devcontainer configuration"
			}
		],
		"snippets": [
			{
				"language": "java",
				"path": "./snippets/java.json"
			},
			{
				"language": "yaml",
				"path": "./snippets/yaml.json"
			},
			{
				"language": "quarkus-properties",
				"path": "./snippets/quarkus-properties.json"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "extension.initDevContainer",
					"when": "explorerResourceIsFolder",
					"group": "Quarkus@1"
				}
			]
		}
	},
	"extensionDependencies": [
		"redhat.java"
	],
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"compile": "tsc -p ./",
		"watch": "webpack --mode development --watch",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js",
		"tslint": "tslint -c tslint.json 'src/**/*.ts'"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.17.13",
		"@types/traverse": "^0.6.32",
		"@types/vscode": "^1.41.0",
		"@types/xml2js": "^0.4.5",
		"glob": "^7.1.6",
		"mocha": "^6.2.2",
		"ts-loader": "^6.2.2",
		"tslint": "^5.20.1",
		"typescript": "^3.7.4",
		"vscode-test": "^1.3.0",
		"webpack": "^4.42.1",
		"webpack-cli": "^3.3.11"
	},
	"dependencies": {
		"@types/ncp": "^2.0.3",
		"ncp": "^2.0.0",
		"traverse": "^0.6.6",
		"typed-rest-client": "^1.7.1",
		"xml2js": "^0.4.23"
	},
	"__metadata": {
		"id": "09e0c57a-8f44-4097-be54-62ce0fe71495",
		"publisherId": "63cef066-e05b-4984-9f0e-a9fa206f0773",
		"publisherDisplayName": "Geoffrey GREBERT",
		"installedTimestamp": 1619185418549
	}
}