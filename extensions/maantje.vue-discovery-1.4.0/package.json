{
	"name": "vue-discovery",
	"displayName": "Vue discovery",
	"description": "This extension discovers Vue components in your workspace and provides IntelliSense for them.",
	"publisher": "maantje",
	"repository": "https://github.com/maantje/vue-discovery",
	"version": "1.4.0",
	"icon": "images/icon.png",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./src/extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Vue discovery",
			"properties": {
				"vueDiscovery.rootDirectory": {
					"type": "string",
					"default": "/src",
					"description": "root directory in your workspace"
				},
				"vueDiscovery.componentCase": {
					"type": "string",
					"enum": [
						"pascal",
						"kebab"
					],
					"default": "pascal",
					"description": "The casing of the imported component"
				},
				"vueDiscovery.addTrailingComma": {
					"type": "boolean",
					"default": true,
					"description": "Add a trailing comma"
				},
				"vueDiscovery.propCase": {
					"type": "string",
					"enum": [
						"camel",
						"kebab"
					],
					"default": "kebab",
					"description": "The casing of the props"
				}
			}
		},
		"keybindings": [
			{
				"command": "vueDiscovery.importExisting",
				"key": "ctrl+i",
				"mac": "cmd+i",
				"when": "editorTextFocus"
			}
		],
		"commands": [
			{
				"command": "vueDiscovery.importExisting",
				"title": "Vue Discovery: Import a vue component"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.44.0",
		"eslint": "^6.8.0",
		"eslint-config-spatie": "^2.0.3",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"glob": "^7.1.6",
		"lodash": "^4.17.15",
		"vue-parser": "^1.1.6"
	},
	"__metadata": {
		"id": "27ad0b81-dfab-443f-bc02-dab774e70f0b",
		"publisherId": "2e72c2da-f8ea-44f3-af18-63c3829f47cc",
		"publisherDisplayName": "Maantje"
	}
}